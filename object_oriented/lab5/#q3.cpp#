0#include <iostream>

class Complex{
  int real,imag;
public:
  static int counter;

  Complex(int r, int i){
    real=r;
    imag=i;
    counter++;

  }

  static void displayObj(){
    std::cout<<"Total objects created are:"<<counter<<"\n";
  }

  
  
};
void test(){
    Complex c(0,1);
  }

int Complex::counter=0;

int main(){
  Complex c1(3,2);
  Complex c3(4,1);
  test();
  Complex::displayObj();


  std::cout<<Complex::counter;
}
